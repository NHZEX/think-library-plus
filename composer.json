{
    "name": "zxin/think-library-plus",
    "type": "library",
    "require": {
        "php": "^8.0",
        "composer-runtime-api": "^2",
        "brick/varexporter": "^0.4.0",
        "composer/pcre": "^3.1",
        "symfony/finder": "^6.0",
        "symfony/var-exporter": "^6.0",
        "topthink/framework": "^8.0.2",
        "topthink/think-helper": "^3.1.6",
        "zxin/utils": "^2.2"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "nette/php-generator": "^4.0",
        "php-cs-fixer/shim": "^3.54",
        "phpstan/phpstan": "^1.10.67",
        "phpunit/phpunit": "^9.6",
        "rector/rector": "^1.0.4",
        "topthink/think-orm": "~3.0.11"
    },
    "autoload": {
        "psr-4": {
            "Zxin\\Think\\": [
                "src/",
                "legacy/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "example\\": "example/"
        }
    },
    "suggest": {
        "nette/php-generator": "ModelGenerator require ^4.0"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        }
    },
    "bin": [
        "bin/dump-auth",
        "bin/dump-route",
        "bin/dump-validate"
    ],
    "extra": {
        "think": {
            "services": [
            ],
            "config": {
                "auth": "config/auth.php",
                "validate": "config/validate.php"
            }
        }
    }
}
